    "run-sequence": "^2.2.1",
    "style-loader": "^1.2.1",
    "through2": "^2.0.1",
    "webpack": "^4.6.0",
    "webpack-cli": "^3.3.11",
    "webpack-glsl-loader": "^1.0.1"
  },
  "engines": {
    "node": "^10.0.0"
  },
  "dependencies": {
    "@fancyapps/fancybox": "^3.5.7",
