    "concurrently": "^5.2.0",
    "gulp-hash": "^4.2.2",
    "gulp-hash-references": "^3.0.1",
    "js-yaml": "^3.12.1",
    "luxon": "^1.24.1",
    "netlify-cli": "^2.48.0",
    "svg4everybody": "^2.1.9",
    "swiper": "^4.4.6",
    "webpack-bundle-analyzer": "^3.8.0"
  },
  "relativeDependencies": {
    "@focus-reactive/react-app-layer": "../react-app-layer/",
    "@focus-reactive/graphql-content-layer": "../graphql-content-layer/"
  }
}
