{
  "name": "live.jsnation.com",
  "version": "0.2.0",
  "scripts": {
    "build": "gulp build --production",
    "start": "gulp",
    "test:build": "gulp test",
    "test:build-update": "gulp test -u",
    "-precommit": "lint-staged",
    "lint": "eslint --ignore-pattern 'src/js/lib/' --cache --format=node_modules/eslint-formatter-pretty src/js/",
    "lint-fix": "npm run lint -- --fix",
    "test:migration": "yarn jest --watch",
    "test:smoke": "jest __tests__/smoke.test.js",
    "storybook": "start-storybook -p 6006 --ci",
    "build-storybook": "build-storybook",
    "deploy:storybook": "echo 'skip storybook'",
    "deploy:storybook_": "storybook-to-ghpages",
    "prestart": "relative-deps",
    "dev:content": "yarn prestart && yarn storybook"
  },
  "lint-staged": {
    "src/templates/**/*.html": [
      "htmlhint --config .htmlhintrc"
    ]
  },
